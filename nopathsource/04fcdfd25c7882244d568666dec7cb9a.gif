          /**
           *目的：PC端与移动端保持显示的内容相同；
           *税务要闻区域PC端一共9行，分为要闻区域和专区。
           *方法：摘要里录入三位数，默认643，如 ABC，A：{6,7,8,9} ,代表税务要闻占几行（专区则占 9-A 行）；B:取{不大于A}，代表税务新闻有几条，超过的则动态隐藏；C：{0,1,2,3}，专区显示的条数（条数不能小于 9-A）。如：851, 8-税务要闻8行，5-税务要闻显示5条，超出的5条的隐藏，1-专区显示1条，超出1条的隐藏。
           *通过第一条税务要闻摘要来设置 class
           *
           */ 
          function setSwyw(){
            var swywLine,swywLength,swzqLength;
            var tempClassName = 'ml_yaowen_line';
            var dataSwywFlag = $('.ml_meiti_b_top').find('li').eq(0).attr('data-line');
            if(dataSwywFlag == ''){
              dataSwywFlag = '643';
              // console.log('---------未设置')
            }else if(dataSwywFlag.length !== 3){
              dataSwywFlag = '643';
              // console.log('---------不是三位数');
            }            
            swywLine = dataSwywFlag[0];
            swywLength = dataSwywFlag[1];
            swzqLength = dataSwywFlag[2];
            //if((swzqLength<(9-swywLine)) || swywLength>swywLine){
            // console.log('---------值有错，未达到临界值');
            // return false;
            // }
            if(swywLine>5&&swywLine<10){
              tempClassName = tempClassName+swywLine;
              $('.ml_yaowen').addClass(tempClassName);
              $('.ml_meiti_b_top li:gt('+(swywLength-1)+')').hide();
              $('.ml_meiti_b_middle li:gt('+(swzqLength-1)+')').hide();
            }
          }
        });
      </script>


